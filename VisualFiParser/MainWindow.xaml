        <Label x:Name="label_Copy5" Content="Città" HorizontalAlignment="Left" Margin="66,93,0,0" VerticalAlignment="Top" Width="104" Height="26"/>
        <Label x:Name="label_Copy6" Content="Prov" HorizontalAlignment="Left" Margin="394,93,0,0" VerticalAlignment="Top" Width="84" Height="26"/>
