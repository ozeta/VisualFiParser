    <Grid Margin="0,0,0,19">
        <Grid.RowDefinitions>
        </Grid.RowDefinitions>
        <Label FontSize="18" x:Name="label" Content="Dati Squadra" HorizontalAlignment="Left" Margin="26,10,0,0" VerticalAlignment="Top" Width="158" Height="36"/>
        <Label x:Name="label_Copy1" Content="Nome" HorizontalAlignment="Left" Margin="66,62,0,0" VerticalAlignment="Top" Width="104" Height="26"/>
        <Label x:Name="label_Copy2" Content="Indirizzo" HorizontalAlignment="Left" Margin="66,124,0,0" VerticalAlignment="Top" Width="104" Height="26"/>
